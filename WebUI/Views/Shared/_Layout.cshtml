<body class="page-container-bg-solid">
    <!-- BEGIN HEADER -->
    <div class="page-wrapper">
        <div class="page-wrapper-row">
            <div class="page-wrapper-top">
                <!-- BEGIN HEADER -->
                <div class="page-header">
                    <!-- BEGIN HEADER TOP -->
                    <div class="page-header-top">
                        <div class="container">
                            <!-- BEGIN LOGO -->
                            <div class="page-logo" style="height:auto;">
                                <a href="">
                                    <img style="margin: 0px; width: 270px !important; margin: 5px !important; margin-left: 0px !important;" src="Content/img/logo.png" alt="logo" class="logo-default">
                                </a>
                            </div>
                            <!-- END LOGO -->
                            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
                            <a href="javascript:;" class="menu-toggler"></a>
                            <!-- END RESPONSIVE MENU TOGGLER -->
                            <!-- BEGIN TOP NAVIGATION MENU -->
                            <div class="top-menu">
                                <ul class="nav navbar-nav pull-right">
                                    <!-- BEGIN NOTIFICATION DROPDOWN -->
                                    <!-- DOC: Apply "dropdown-hoverable" class after "dropdown" and remove data-toggle="dropdown" data-hover="dropdown" data-close-others="true" attributes to enable hover dropdown mode -->
                                    <!-- DOC: Remove "dropdown-hoverable" and add data-toggle="dropdown" data-hover="dropdown" data-close-others="true" attributes to the below A element with dropdown-toggle class -->
                                    <!-- END TODO DROPDOWN -->
                                    <li class="droddown dropdown-separator">
                                        <span class="separator"></span>
                                    </li>
                                    <!-- BEGIN INBOX DROPDOWN -->
                                    <!-- END INBOX DROPDOWN -->
                                    <!-- BEGIN USER LOGIN DROPDOWN -->

                                    @if (SessionRequest._User != null && SessionRequest._User.Id > 0)
                                    {
                                        <li class="dropdown dropdown-user dropdown-dark">
                                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                                <img alt="" class="img-circle" src="Content/img/avatar.png">
                                                <span class="username username-hide-mobile">

                                                    @SessionRequest._User.Name @SessionRequest._User.Surname

                                                </span>
                                            </a>
                                            <ul class="dropdown-menu dropdown-menu-default">

                                                <li>
                                                    <a href="User/ChangePassword?id=@SessionRequest._User.Id" data-target="#ajax" data-toggle="modal">
                                                        <i class="icon-user"></i>  @Html.Raw("Change Password".Trans())
                                                    </a>
                                                </li>

                                                <li>
                                                    <a href="Login/LogOff">
                                                        <i class="icon-key"></i> @Html.Raw("Exit".Trans())
                                                    </a>
                                                </li>
                                            </ul>
                                        </li>

                                        <!-- END USER LOGIN DROPDOWN -->
                                    }



                                    <!-- END QUICK SIDEBAR TOGGLER -->
                                </ul>
                            </div>
                            <!-- END TOP NAVIGATION MENU -->
                        </div>
                    </div>
                    <!-- END HEADER TOP -->
                    <!-- BEGIN HEADER MENU -->

                    <div class="page-header-menu" style="display: block;">
                        <div class="container">
                            <div class="hor-menu">
                                <ul class="nav navbar-nav">
                                    <li aria-haspopup="true" class="menu-dropdown classic-menu-dropdown active"><a href="User"><span class="arrow">Users</span></a></li>

                                </ul>
                            </div>
                            <!-- END MEGA MENU -->
                        </div>
                    </div>

                    <!-- END HEADER MENU -->
                </div>
                <!-- END HEADER -->
            </div>
        </div>
        <div class="page-wrapper-row full-height">
            <div class="page-wrapper-middle">
                <!-- BEGIN CONTAINER -->
                <div class="page-container">
                    <!-- BEGIN CONTENT -->
                    <div class="page-content-wrapper">
                        <!-- BEGIN CONTENT BODY -->
                        <!-- BEGIN PAGE HEAD-->
                        @*<div class="page-head">
                                <div class="container">
                                    <!-- BEGIN PAGE TITLE -->
                                    <div class="page-title">
                                        <h1>
                                            Dashboard
                                            <small>dashboard &amp; statistics</small>
                                        </h1>
                                    </div>
                                    <!-- END PAGE TITLE -->
                                </div>
                            </div>*@
                        <!-- END PAGE HEAD-->
                        <!-- BEGIN PAGE CONTENT BODY -->
                        <div class="page-container page-content-inner page-container-bg-solid">
                            @RenderBody()
                        </div>

                        <!-- END PAGE CONTENT BODY -->
                        <!-- END CONTENT BODY -->
                    </div>
                    <!-- END CONTENT -->
                </div>
                <!-- END CONTAINER -->
            </div>
        </div>


        <div class="copyright font-blue-dark copyrightCustom">
            @Html.Raw(SessionRequest.Copyright)
            <hr>
        </div>

    </div>

    <div class="modal fade" id="ajax" role="dialog" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <img src="@SessionRequest.baseUrl/assets/global/img/loading-spinner-grey.gif" alt="" class="loading">
                    <span> &nbsp;&nbsp;@Html.Raw("Loading".Trans())... </span>
                </div>
            </div>
        </div>
    </div>

  

